"Vulncode":"...\n	<manifest xmlns:android=\"http://schemas.android.com/apk/res/android\"\n package = \"com.example.android.apis\"\n	\tandroid:versionCode=\"1\"\n   \tandroid:versionName=\"10\"\n\t\tandroid:sharedUserId=\"android.uid.developer1\">",
"Secucode":"...\n	<manifest xmlns:android=\"http://schemas.android.com/apk/res/android\"\n package = \"com.example.android.apis\"\n	\tandroid:versionCode=\"1\"\n   \tandroid:versionName=\"10\"\n>"}